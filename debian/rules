#!/usr/bin/make -f

export DH_VERBOSE=1

include /usr/share/cdbs/1/rules/debhelper.mk
include /usr/share/cdbs/1/class/qmake.mk

QMAKE = /usr/lib/x86_64-linux-gnu/qt5/bin/qmake

TARGET = quimup
target.path = /usr/bin
INSTALLS += target

/usr/bin/quimup:: quimup 
	echo "####### start $< > $@"
	[ -f $@ ]  || install $< $@
	echo "####### end   $< > $@"

